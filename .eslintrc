{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0
  },
  "globals": {
    "log": false,
    "__DEV__": true,
    "__LOG_LEVEL__": true,
    "__VERSION__": false
  }
}